<% @publications.each do |publication| %>
  <% present publication, PublicationPresenter do |p| %>
    <div class="publication">
      <h2><%= p.link_to_page %></h2>
      <p><em>Published on <%= p.published %>  by <%= p.author %> - <%= p.type %></em></p>
      <p><%= p.excerpt %></p>
    </div>
  <% end %>
<% end %>
